          {"eventName": "STORE:DOCUMENTHASH", "callbackUrl": "http://webapp-dtag:3000/api/v1/documents/event"},
          {"eventName": "STORE:SIGNATURE", "callbackUrl": "http://webapp-dtag:3000/api/v1/signatures/event"}
